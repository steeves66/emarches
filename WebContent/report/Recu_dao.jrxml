	<queryString language="SQL">
		<![CDATA[select distinct dc.dac_code,v.ven_paie_code,dv.dve_cout,dv.dve_cout_lettre,can.can_nom ||' ' ||can.can_prenoms candidat,can.can_tel,f.fon_libelle||' '||op.ope_matricule fonlibelle,hd.hac_date
from t_dac_specs dc, t_candidats can, t_vente_dac v, t_detail_vente dv, t_histo_dac hd,t_fonction f, t_assignation a, t_operateur op
where dc.dac_code = hd.hac_dac_code
and hd.hac_fon_cod = f.fon_cod
and can.can_code = v.ven_can_code
and v.ven_num = dv.dve_ven_num
and dc.dac_code = dv.dve_dac_code
and hd.hac_fon_cod = a.ass_fon_cod
and a.ass_statut = 1
and a.ass_ope_matricule = op.ope_matricule
and hd.hac_sta_code = 'DVE'
and dc.dac_code = $P{param_code_1}]]>
