	<queryString language="SQL">
		<![CDATA[select af.*,tym.tym_libelle_long,tym.tym_libelle_court,mop.mop_libelle_long ,f.fon_libelle, min.min_libelle,st.str_code,st.str_libelle_court,st.str_libelle_long ,sf.sou_libelle, fg.fip_montant_cfa,fg.fip_montant_devise,ba.bai_libelle,dev.dev_libelle,dev.dev_symbole
from t_affichage_pgpm af, t_type_marche tym, t_mode_passation mop, t_fonction f, t_ministere min,t_structure st, t_financement_pgpm fg,t_source_financement sf, t_bailleur ba, t_devise dev
where af.aff_gpg_tym_code = tym.tym_code
and af.aff_gpg_mop_code = mop.mop_code
and af.aff_gpg_fon_cod = f.fon_cod
and af.aff_gpg_str_code = st.str_code
and st.str_min_code = min.min_code
and af.aff_gpg_id = fg.fip_gpg_id (+)
and af.aff_gpg_source_fin = sf.sou_code
and fg.fip_sou_code = sf.sou_code
and fg.fip_bai_code = ba.bai_code
and fg.fip_dev_code = dev.dev_code 
and af.aff_gpg_id=  $P{param_code}]]>
	</queryString>
