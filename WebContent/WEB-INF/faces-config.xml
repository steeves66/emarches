<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
    <validator>
  <validator-id>validatorExt</validator-id>
  <validator-class>com.sndi.validator.ValidatorExt</validator-class>
 </validator>
 
  <validator>
  <validator-id>validatorCSV</validator-id>
  <validator-class>com.sndi.validator.ValidatorCSV</validator-class>
 </validator>
    <application>
        <el-resolver>
            org.springframework.web.jsf.el.SpringBeanFacesELResolver
        </el-resolver>
    </application>
    <application>
		 <resource-bundle>
		 <base-name>
		 messages.messages
		 </base-name>
		 <var>msg</var>
		 </resource-bundle>
 </application>

</faces-config>
